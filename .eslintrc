{
  "extends": "eslint:recommended",
  "rules": { //Put all rules that we want to throw a warning if we don't do these
    "no-alert": 1,
    "camelcase": 1,
    "curly": 1,
    "eqeqeq": 1,
    "no-console": 0,
    "guard-for-in": 1,
    "no-empty": 1,
    "no-param-reassign": 1,
    "no-unused-vars": 1,
    "quotes": [ 0, "single"]
  },
  "globals": {
    // any globals we want to allow will go here
    "jQuery": false,
    "$": false
  },
  "env": {
    "browser": true
  }
}